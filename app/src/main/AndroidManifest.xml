<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.contaminacion.madrid.contaminacionmadrid">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContMadridActivity"
            android:label="@string/activity_contMadrid" />
        <activity
            android:name=".AvisosContaminacion"
            android:label="@string/activity_avisos_contaminacion" />/>
        <activity
            android:name=".ComparadorActivity"
            android:label="@string/activity_comparador"/>
        <activity
            android:name=".RankingActivity"
            android:label="@string/activity_ranking"/>
        <activity
            android:name=".SportsActivity"
            android:label="@string/activity_sports"/>
        <activity
            android:name=".EPOCActivity"
            android:label="@string/activity_EPOC" />
    </application>

</manifest>